import java.util.Collections;

public class prac{
	
	
	public static void main(String args[]) {
		//reverse array
		Integer max=0,min=0;
		Integer [] arr= {2,56,2,8,1,4};
		min=max=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(max<arr[i])
				max=arr[i];
			else if(min>arr[i])
				min=arr[i];
		}
		System.out.println("max element : "+max+" min element : "+min);
	}
}
